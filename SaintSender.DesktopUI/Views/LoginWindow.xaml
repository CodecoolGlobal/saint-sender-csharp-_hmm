<Window x:Class="SaintSender.DesktopUI.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="LoginWindow" Height="400" Width="400">
    <Grid>
        <TextBox x:Name="emailInput" Width="300" HorizontalAlignment="Left" Margin="51,88,0,238.4"/>
        <PasswordBox x:Name="passwordInput" Width="300" HorizontalAlignment="Left" Margin="51,181,0,161.4"/>
        <Button Name="loginBtn" Content="Login" HorizontalAlignment="Right" VerticalAlignment="Top" Width="93" Margin="0,273,249.6,0" Height="37"/>
        <Button Content="Cancel" HorizontalAlignment="Left" VerticalAlignment="Top" Width="93" Margin="258,273,0,0" Height="37"/>
    </Grid>
</Window>
